<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CancelableDialog">
        <attr name="android:gravity" />
        <attr name="dialogFixedWidthMajor" format="dimension|fraction" />
        <attr name="dialogFixedWidthMinor" format="dimension|fraction" />
        <attr name="dialogFixedHeightMajor" format="dimension|fraction" />
        <attr name="dialogFixedHeightMinor" format="dimension|fraction" />
    </declare-styleable>

    <declare-styleable name="AutoTurnViewPager">
        <attr name="bv_canLoop" format="boolean" />
        <attr name="bv_canTurn" format="boolean" />
        <attr name="bv_autoTurnTime" format="integer|reference" />
        <attr name="bv_scrollDuration" format="integer|reference" />
        <attr name="bv_isTouchScroll" format="boolean" />
        <attr name="bv_reverse" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ForegroundImageView">
        <attr name="android:foreground" />
        <attr name="android:foregroundGravity" />
    </declare-styleable>
</resources>
